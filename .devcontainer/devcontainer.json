{
  "name": "Mnemonic Core Dev Environment",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "mnemonic-core",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "features": {
    "ghcr.io/devcontainers/features/rust:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "serayuzgur.crates"
      ]
    }
  }
}
